<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" prefHeight="309.0" prefWidth="449.0" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AboutDialog">
   <children>
      <ImageView fx:id="if_logo" fitHeight="299.0" fitWidth="280.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </ImageView>
      <Label fx:id="lbl_versionAndCopyright" text="TotalBackup v0.1pre5 beta&#10;Copyright 2014-2018 Andreas Fleig - Alle Rechte vorbehalten&#10;Lizenz: GPLv3+" textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <Button mnemonicParsing="false" onAction="#OKAction" text="OK">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
